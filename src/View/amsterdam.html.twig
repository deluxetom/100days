<!-- Bootstrap -->
<link href="/bower_components/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
<style type="text/css">
@import url(https://fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,800,700italic,800italic);
body { background: #312f2c; color: #fff; font-family: "Open Sans", Impact, Haettenschweiler, "Franklin Gothic Bold", "Arial Black", sans-serif;  font-size: 16px; }
.font75 { font-size: 75%; }
#heading { font-size: 8.5rem; padding-top: 50px; }
.pale { color: #bebdbb; }
.strong { font-weight: 700; }
.stronger { font-weight: 800; }
#ac {
